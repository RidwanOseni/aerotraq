# === LLAMA Cloud API ===
LLAMA_CLOUD_API_KEY=<your_llama_cloud_api_key>

# === IPFS Configuration ===
IPFS_HOST=127.0.0.1
IPFS_PORT=5001
IPFS_PROTOCOL=http
IPFS_API_PATH=/api/v0

# === Database Configuration ===
DB_PATH=flight_data.db

# === Flight Data Configuration ===
MAX_FLIGHT_DURATION=120
MAX_ALTITUDE=400

# === Web3 Configuration ===
WEB3_PROVIDER_URL=http://localhost:8545
CHAIN_ID=1337

# === Logging Configuration ===
LOG_LEVEL=INFO
LOG_FILE=app.log

# === OpenAI API Key ===
OPENAI_API_KEY=<your_openai_api_key>

# === OpenAIP Integration ===
OPENAIP_API_KEY=<your_openaip_api_key>
OPENAIP_SERVER_PATH=<absolute_or_relative_path_to_openaip_server_script>
